  <concept id="john-lakos-metrics:PackagesDependsUponPackageA" severity="major">
    <requiresConcept refId="john-lakos-metrics:PackagesDependsUpon"/>
    <description>Counts all packages this package depends upon inclusive the package itself.</description>
    <cypher><![CDATA[
      MATCH (p:Package {fqn:"your.project.johnlakos.a", dependsUpon:1})
      RETURN p
    ]]></cypher>
  </concept>

